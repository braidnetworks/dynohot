{
	"name": "dynohot",
	"type": "module",
	"version": "1.0.1",
	"exports": {
		".": "./dist/loader/loader.js",
		"./?*": "./dist/loader/loader.js?*",
		"./import-meta": {
			"types": "./import-meta.d.ts"
		},
		"./node-loader": {
			"import": "./dist/loader/node-loader.d.ts",
			"types": "./loader/node-loader.js"
		},
		"./*": {
			"import": "./dist/*.js",
			"types": "./dist/*.d.ts"
		}
	},
	"scripts": {
		"lint": "eslint --max-warnings=0 --report-unused-disable-directives .",
		"prepare": "rm -rf dist && tsc -b",
		"test": "NODE_OPTIONS='--no-warnings --experimental-vm-modules' npx jest --silent"
	},
	"dependencies": {
		"@babel/core": "^7.23.2",
		"@babel/generator": "^7.23.0",
		"@babel/traverse": "^7.23.2",
		"@stylistic/eslint-plugin": "^0.1.1",
		"convert-source-map": "^2.0.0"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.22.15",
		"@babel/preset-env": "^7.23.2",
		"@babel/preset-typescript": "^7.23.2",
		"@eslint/js": "^8.52.0",
		"@jest/globals": "^29.7.0",
		"@jest/types": "^29.6.3",
		"@types/babel__core": "^7.20.3",
		"@types/babel__generator": "^7.6.6",
		"@types/babel__traverse": "^7.20.3",
		"@types/convert-source-map": "^2.0.2",
		"@types/node": "^20.8.9",
		"@typescript-eslint/eslint-plugin": "^6.9.0",
		"@typescript-eslint/parser": "^6.9.0",
		"babel-jest": "^29.7.0",
		"babel-plugin-transform-import-meta": "^2.2.1",
		"eslint": "^8.52.0",
		"eslint-plugin-import": "^2.29.0",
		"globals": "^13.23.0",
		"jest": "^29.7.0",
		"typescript": "^5.2.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/braidnetworks/dynohot.git"
	},
	"author": "https://github.com/braidnetworks/",
	"license": "ISC",
	"homepage": "https://github.com/braidnetworks/dynohot#readme"
}
